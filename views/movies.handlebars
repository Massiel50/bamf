<div class="container">
  {{!-- search bar --}}
  <div class="container" <!-- search bar-->
    <div class="row">
      <div class="input-field col s12">
        <i class="material-icons prefix">search</i>
        <input type="text" id="search autocomplete-input" class="autocomplete searchMovie">
        <label for="autocomplete-input">SEARCH MOVIES</label>
      </div>
      <a id="searchBtn" rel="noopener noreferrer"class="waves-effect waves-light btn">Search</a>
    </div>
    
   
    <h1>Movie title: {{results}}</h1>
    <p>Movie image:{{results}}</p>
    <p>Movie plot:{{results}}</p>




    <!-- cards for movies -->
    <div class="row movieCard">
      <div class="col s3">
        <div class="card">
          <div class="card-image">
            <img src="/public/styles/images/b&w tree.jpg">

            <a class="btn-floating halfway-fab waves-effect waves-light red"><i class="material-icons">add</i></a>
          </div>
          <div class=" moviePoster card-content">
            <span class="card-title">Card Title</span>
          </div>
        </div>
      </div>

    </div>