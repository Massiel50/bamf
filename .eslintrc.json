{
  "env": {
      "browser": true,
      "commonjs": true,
      "node": true
  },
  "rules": {
      "no-duplicate-case":"warn",
      "no-empty":"error",
      "no-extra-semi":"warn",
      "no-func-assign": "error",
      "no-irregular-whitespace":"warn",
      "no-unreachable": "error",
      "curly": "warn",
      "dot-notation": "warn",
      "eqeqeq": "error",
      "no-empty-function": "error",
      "no-multi-spaces": "warn",
      "no-mixed-spaces-and-tabs": "warn",
      "no-trailing-spaces": "off",
      "default-case": "warn",
      "no-fallthrough": "warn",
      "no-unused-vars": "error",
      "no-use-before-define": "error",
      "no-redeclare": "error",
      "camelcase": "warn",
      "brace-style": "warn",
      "indent": [
          "warn",
          2
      ],
      "linebreak-style": [
          "warn",
          "unix"
      ],
      "quotes": [
          "error",
          "double"
      ],
      "semi": [
          "error",
          "always"
      ]
  }
}
